package day5;

public class forLoopDemo5 {

public static void main(String[] args) {
		
		
			
			System.out.println("Start");
			
			for(int i=0;i<=100;i=i+3)
			{
				System.out.println(i);
				
				if(i==9)
				{
					System.out.println("Found");
					break;
				}	
			}
			
			System.out.println("End");
			
		}

	}