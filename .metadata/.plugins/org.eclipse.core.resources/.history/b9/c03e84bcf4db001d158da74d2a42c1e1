public static void main(String[] args) {
	
	//System.out.println("Start");
	
	for(int i=5;i>=0;i--)
	{
		for(int j=1;j<=i;j++)
		{
			System.out.print("*");
	
		}
		System.out.println();
	}
	
	//System.out.println("End");
}

}
