package java_practise;

import java.util.Scanner;

public class PrintingStar {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int a=0;
		
		for (int i =1;i<=0;i++) {
			for(int j=1; j<=i;j++)
			{
				System.out.println("*");
			}
			
			System.out.println();
		}

	}

}
